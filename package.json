{
  "name": "@swartdraak/proxmox-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for Proxmox VE management via AI assistants",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "proxmox-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run start",
    "lint": "tsc --noEmit",
    "test": "echo \"No tests\"",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "prepublishOnly": "npm run clean && npm run build",
    "prepack": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "proxmox",
    "proxmox-ve",
    "virtualization",
    "ai",
    "llm",
    "claude",
    "github-copilot",
    "ide-integration",
    "typescript",
    "cross-platform"
  ],
  "author": "Swartdraak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Swartdraak/Proxmox-MCP.git"
  },
  "bugs": {
    "url": "https://github.com/Swartdraak/Proxmox-MCP/issues"
  },
  "homepage": "https://github.com/Swartdraak/Proxmox-MCP#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.13.5",
    "https-proxy-agent": "^7.0.5"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "typescript": "^5.7.3"
  }
}
